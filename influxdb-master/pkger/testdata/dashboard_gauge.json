[
    {
        "apiVersion": "influxdata.com/v2alpha1",
        "kind": "Dashboard",
        "metadata": {
            "name": "dash-1"
        },
        "spec": {
            "description": "desc1",
            "charts": [
                {
                    "kind": "gauge",
                    "name": "gauge",
                    "prefix": "prefix",
                    "tickPrefix": "true",
                    "suffix": "suffix",
                    "tickSuffix": "false",
                    "note": "gauge note",
                    "noteOnEmpty": true,
                    "xPos": 1,
                    "yPos": 2,
                    "width": 6,
                    "height": 3,
                    "decimalPlaces": 1,
                    "xColumn": "_time",
                    "yColumn": "_value",
                    "queries": [
                        {
                            "query": "from(bucket: v.bucket)  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)  |> filter(fn: (r) => r._measurement == \"boltdb_writes_total\")  |> filter(fn: (r) => r._field == \"counter\")"
                        }
                    ],
                    "colors": [
                        {
                            "name": "laser",
                            "type": "min",
                            "hex": "#8F8AF4",
                            "value": 0
                        },
                        {
                            "name": "pool",
                            "type": "threshold",
                            "hex": "#F4CF31",
                            "value": 700
                        },
                        {
                            "name": "comet",
                            "type": "max",
                            "hex": "#F4CF31",
                            "value": 5000
                        }
                    ]
                }
            ]
        }
    }
]
